{
  "name": "lostcity-quiz",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "scrape": "tsx scripts/scrape.ts",
    "scrape:historical": "tsx scripts/scrape-historical-images.ts",
    "scrape:lengths": "tsx scripts/scrape-lengths.ts",
    "validate-data": "tsx scripts/validate-data.ts",
    "regenerate-json": "tsx scripts/regenerate-json.ts",
    "emulators": "firebase emulators:start --only auth,firestore",
    "test:e2e": "firebase emulators:exec --only auth,firestore 'npx playwright test'",
    "deploy": "npm run build && firebase deploy --only hosting"
  },
  "dependencies": {
    "firebase": "^12.9.0",
    "vue": "^3.5.25",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/tsconfig": "^0.8.1",
    "firebase-tools": "^15.6.0",
    "sharp": "^0.33.0",
    "tsx": "^4.19.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vue-tsc": "^3.1.5"
  }
}
